{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "build": {
    "content": [
      {
        "files": [
          "docs/**/*.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/**/*.png",
          "docs/**/*.jpg",
          "docs/**/*.svg",
          "static/**/*.css"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "modern",
      "default"
    ],
    "globalMetadata": {
      "_appTitle": "Technical Documentation",
      "_appName": "Docs Multi-Stack",
      "_appFooter": "Generated by dotnet-angular-blazor-docs skill",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
